<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="258.0" prefWidth="324.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nttd.wtdoodle.Client.Lobby.GameCreator">
   <children>
      <Label layoutX="125.0" layoutY="21.0" text="Game Data" />
      <Label layoutX="29.0" layoutY="66.0" text="Max Players">
         <graphic>
            <TextField fx:id="tf_maxPlayers" promptText="Max 8 players" />
         </graphic>
      </Label>
      <Label layoutX="29.0" layoutY="111.0" text="Guessing Time">
         <graphic>
            <TextField fx:id="tf_guessingTime" promptText="Time in sec" />
         </graphic>
      </Label>
      <Button fx:id="btn_createGame" layoutX="112.0" layoutY="203.0" mnemonicParsing="false" text="Create Game" />
      <Label layoutX="29.0" layoutY="152.0" text="Rounds">
         <graphic>
            <TextField fx:id="tf_numRounds" promptText="Time in sec" />
         </graphic>
      </Label>
   </children>
</AnchorPane>
